{
  "name": "metronomejs",
  "version": "1.0.0",
  "description": "Node & Browser wrappers for Metronome Token",
  "main": "dist/metronome.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --colors -p",
    "dev": "webpack --progress --colors --watch",
    "test": "jest",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "eslint ./src --fix .js",
    "testrpc": "testrpc",
    "sample:node": "nodemon examples/node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MetronomeToken/metronomejs.git"
  },
  "keywords": [
    "js",
    "node",
    "browser",
    "token",
    "metronome"
  ],
  "bugs": {
    "url": "https://github.com/MetronomeToken/metronomejs/issues"
  },
  "homepage": "https://github.com/MetronomeToken/metronomejs#readme",
  "dependencies": {
    "solc": "^0.4.17",
    "trae": "^1.4.2",
    "web3": "^1.0.0-beta.23"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "ethereumjs-testrpc": "^4.1.3",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2",
    "webpack": "^3.8.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
